{
    "name": "PyInventory",
    "id": "pyinventory",
    "packaging_format": 1,
    "description": {
        "en": "Web based management to catalog things including state and location etc."
    },
    "version": "0.9.4~ynh5",
    "url": "https://github.com/jedie/PyInventory",
    "upstream": {
        "license": "GPL-3.0",
        "code": "https://github.com/jedie/PyInventory"
    },
    "license": "GPL-3.0",
    "maintainer": {
        "name": "Jens Diemer",
        "email": "pyinventory_ynh@jensdiemer.de"
    },
    "requirements": {
        "yunohost": ">= 4.1.3"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "domain.org"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/pyinventory",
                "default": "/pyinventory"
            },
            {
                "name": "admin",
                "type": "user",
                "example": "johndoe"
            }
        ]
    }
}
